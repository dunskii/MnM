# Music 'n Me Frontend - Production Environment Variables
# Copy this file to .env.production and fill in actual values
# These variables are embedded at BUILD TIME by Vite

# ===================================
# API Configuration
# ===================================
VITE_API_URL=https://musicnme.com.au/api/v1
VITE_API_TIMEOUT=30000  # 30 seconds

# ===================================
# Stripe Configuration
# ===================================
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key_here

# ===================================
# Google OAuth Configuration
# ===================================
VITE_GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com

# ===================================
# Application Configuration
# ===================================
VITE_APP_NAME=Music 'n Me
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production

# ===================================
# Feature Flags
# ===================================
VITE_FEATURE_MEET_AND_GREET=true
VITE_FEATURE_HYBRID_LESSONS=true
VITE_FEATURE_GOOGLE_DRIVE=true
VITE_FEATURE_STRIPE_PAYMENTS=true

# ===================================
# UI Configuration
# ===================================
VITE_DEFAULT_THEME=light
VITE_DATE_FORMAT=DD/MM/YYYY  # Australian date format
VITE_TIME_FORMAT=h:mm A
VITE_TIMEZONE=Australia/Sydney

# ===================================
# File Upload Limits
# ===================================
VITE_MAX_FILE_SIZE=10485760  # 10MB in bytes
VITE_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/gif
VITE_ALLOWED_DOCUMENT_TYPES=application/pdf
VITE_ALLOWED_AUDIO_TYPES=audio/mpeg,audio/wav

# ===================================
# Pagination & Data Limits
# ===================================
VITE_DEFAULT_PAGE_SIZE=50
VITE_MAX_PAGE_SIZE=100
VITE_CALENDAR_EVENTS_LIMIT=1000

# ===================================
# Error Tracking (Optional)
# ===================================
# VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
# VITE_SENTRY_ENVIRONMENT=production
# VITE_SENTRY_TRACES_SAMPLE_RATE=0.1

# ===================================
# Analytics (Optional)
# ===================================
# VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
# VITE_MIXPANEL_TOKEN=your-mixpanel-token

# ===================================
# Contact & Support
# ===================================
VITE_SUPPORT_EMAIL=support@musicnme.com.au
VITE_SUPPORT_PHONE=+61-xxx-xxx-xxx
VITE_COMPANY_NAME=Music 'n Me
VITE_COMPANY_ABN=xx xxx xxx xxx

# ===================================
# Social Media Links
# ===================================
VITE_FACEBOOK_URL=https://facebook.com/musicnme
VITE_INSTAGRAM_URL=https://instagram.com/musicnme

# ===================================
# Development Tools
# ===================================
# Disable in production
VITE_ENABLE_DEVTOOLS=false
VITE_ENABLE_LOGGING=false
VITE_ENABLE_DEBUG=false
